{
    "angular-schematics.schematicsDefaultOptions": {
        "angular-*": {
            "externalTemplate": true,
            "style": "scss"
        },
        "angular-component": {
            "skipChangeDetectionOnPush": true,
            "skipDisplayBlock": true
        },
        "angular-page": {
            "skipChangeDetectionOnPush": true
        }
    },
    "typescript.preferences.autoImportFileExcludePatterns": [
        "src/environments/environment.*.ts",
    ]
}